{
  "suggest.echodocSupport": true,
  "suggest.maxCompleteItemCount": 10,
  "eslint.enable": true,
  "prettier.eslintIntegration": true,
  "eslint.filetypes": ["javascript", "javascriptreact", "typescript", "typescriptreact"],
  "coc.preferences.formatOnSaveFiletypes": ["javascript", "javascriptreact", "typescript", "typescriptreact", "reason"],
  "diagnostic.errorSign": "##",
  "diagnostic.warningSign": "!!",
  "diagnostic.infoSign": "ii",
  "typescript.suggest.autoImports": true,
  "javascript.suggest.autoImports": true,
  "snippets.userSnippetsDirectory": "$XDG_CONFIG_HOME/nvim/snippets",
  "explorer.icon.enableNerdfont": true,
  "explorer.keyMappings": {
    "s": "openInVsplit"
  }
}
