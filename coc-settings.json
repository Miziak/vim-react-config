{
  "suggest.echodocSupport": true,
  "suggest.maxCompleteItemCount": 10,
  "eslint.enable": true,
  "prettier.eslintIntegration": true,
  "coc.preferences.formatOnSaveFiletypes": ["javascript", "javascriptreact", "typescript"],
  "diagnostic.errorSign": "##",
  "diagnostic.warningSign": "!!",
  "diagnostic.infoSign": "ii",
  "languageserver": {
    "reason": {
      "command": "~/.rls-linux/reason-language-server",
      "filetypes": ["reason"]
    }
  }
}
